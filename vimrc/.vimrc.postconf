
" NerdTree customization
    " specify initial size of tree pane. Default is 31
    let NERDTreeWinSize=45

set shiftwidth=2
set tabstop=2

let g:gruvbox_contrast_dark = 'hard'
colorscheme gruvbox

let g:rainbow_ctermfgs = [ 'blue', 'cyan', 'yellow', 'magenta' ]
let g:rainbow_conf['ctermfgs'] = g:rainbow_ctermfgs

nmap U <C-r>
